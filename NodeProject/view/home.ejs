        <% for (let student of StudentsDetail) { %>
            <div class="col-md-6 col-lg-4 mb-4">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title"><%= student.name %></h5>
                        <p class="card-text"><strong>Roll No:</strong> <%= student.rollno %></p>
                        <p class="card-text"><strong>Marks:</strong> <%= student.marks %></p>
                        <p class="card-text"><strong>Course:</strong> <%= student.course %></p>
                    </div>
                </div>
            </div>
            <% } %>
</body>
